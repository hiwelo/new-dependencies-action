{
  "name": "new-dependencies-action",
  "version": "1.0.1",
  "private": true,
  "description": "GitHub Action adding a comment with information about new NPM dependencies detected in a pull request",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiwelo/new-dependencies-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Damien Senger <hello@raccoon.studio>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^2.1.1",
    "lodash": "^4.17.19",
    "package-json": "^6.5.0",
    "underscore": "^1.10.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.150",
    "@types/node": "^12.7.12",
    "@types/underscore": "^1.9.4",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "commitlint": "^8.3.5",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
